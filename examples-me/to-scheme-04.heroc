min_of_three(x, y, z) {
	long m;
	if (x < y) {
		if (z < x) {
			m = z;
		} else {
			m = x;
		}
	} else {
		if (z < y) {
			m = z;
		} else {
			m = y;
		}	
	}

	print_long(m);
}

main() {
	print_char('m');
	print_char('i');
	print_char('n');
	print_char('=');
	print_nl();

	min_of_three(1, 2, 3);
	print_nl();
	min_of_three(4, 2, 3);
	print_nl();
	min_of_three(3, 5, 1);
	print_nl();
	min_of_three(4, 3, 2);
	print_nl();

}

